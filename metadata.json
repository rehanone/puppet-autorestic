{
  "name": "rehan-autorestic",
  "version": "0.0.1",
  "author": "Rehan Mahmood",
  "summary": "Install, manage and configure docker/docker-compose management tool autorestic from Github",
  "license": "Apache-2.0",
  "source": "https://github.com/rehanone/puppet-autorestic.git",
  "project_page": "https://github.com/rehanone/puppet-autorestic",
  "issues_url": "https://github.com/rehanone/puppet-autorestic/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 5.0.0 < 9.0.0"
    },
    {
      "name": "puppet-archive",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "stm-file_capability",
      "version_requirement": ">= 3.0.0 < 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
